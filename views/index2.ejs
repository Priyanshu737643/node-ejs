<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <h1>User Admin Interface</h1>
    <p><a href="/adduser">Add User</a></p>
    <ol>
        <% users.forEach(user => { %>
            <li>
                <%= user.name %> -
                <%= user.email %> -
                <%= user.role %>- 
                <a href="/<%= user._id %>/edituser">Edit</a> &nbsp;
                <a href="/<%= user._id %>/deleteuser">Delete</a>
            </li>
        <% }) %>
    </ol>
</body>
</html>
